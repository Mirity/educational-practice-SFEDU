<div class="car">
        <div class="car__item">
            <span>{{ car.id }}</span>
        </div>

        <div class="car__item">
            <span>{{ car.model }}</span>
        </div>

        <div class="car__item">
            <span>{{ car.mileage }}</span>
        </div>

        <div class="car__item">
            <span>{{ car.number }}</span>
        </div>

        <div class="car__item">
            <span>{{ car.brandName }}</span>
        </div>

        <div class="car__item">
            <span>{{ car.countryName }}</span>
        </div>

        <div class="car__item">
            <span>{{ car.clientName }}</span>
        </div>

        <div class="car__item">
            <span>{{ car.clientSurname }}</span>
        </div>

        <div class="car__item">
            <span>{{ car.yearManufacture}}</span>
        </div>

        <div class="car__open">
            <a class="car__link"
               href="/car?id={{ car.id }}"
            >
                Открыть
            </a>
        </div>

        <div class="car__edit">
            <a class="car__link"
               href="/edit-car?id={{ car.id }}"
            >
                Изменить
            </a>
        </div>
</div>